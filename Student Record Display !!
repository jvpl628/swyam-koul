#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int rollnumber;
    char name[100], branch[50], section[10], college[100];

    // Reading inputs
    scanf("%d", &rollnumber);
    getchar();  // Clear newline after integer input

    fgets(name, sizeof(name), stdin);
    fgets(branch, sizeof(branch), stdin);
    fgets(section, sizeof(section), stdin);
    fgets(college, sizeof(college), stdin);

    // Removing trailing newline characters (optional but cleaner)
    name[strcspn(name, "\n")] = '\0';
    branch[strcspn(branch, "\n")] = '\0';
    section[strcspn(section, "\n")] = '\0';
    college[strcspn(college, "\n")] = '\0';

    // Output
    printf("Roll Number: %d\n", rollnumber);
    printf("Name: %s\n", name);
    printf("Branch: %s\n", branch);
    printf("Section: %s\n", section);
    printf("College: %s\n", college);

    return 0;
}


