#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sum(int num) {
    if (num == 0)
        return 0;
    else
        return (num % 10) + sum(num / 10);
}
int main() {
    int num;
    scanf("%d", &num);
    int fact = sum(num);
    printf("%d", fact);
    return 0;
}
