#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int n;
    
    // Input number of elements
    scanf("%d", &n);
    
    int arr[n];
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    int largest = arr[0];
    int second_largest = -10001; // smaller than the minimum possible value
    
    // Find largest element
    for (int i = 1; i < n; i++) {
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }
    
    // Find second largest element
    for (int i = 0; i < n; i++) {
        if (arr[i] > second_largest && arr[i] < largest) {
            second_largest = arr[i];
        }
    }
    
    // Check if second largest exists
    if (second_largest == -10001) {
        printf("No second largest element");
    } else {
        printf("%d", second_largest);
    }
    
    return 0;
}
