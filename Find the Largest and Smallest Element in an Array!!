#include <stdio.h>
#include <string.h>
#include <math.h>#include <stdio.h>

int main() {
    int n;
    
    // Input number of elements
    scanf("%d", &n);
    
    int arr[n];
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Initialize smallest and largest with the first element
    int smallest = arr[0];
    int largest = arr[0];
    
    // Find smallest and largest elements
    for (int i = 1; i < n; i++) {
        if (arr[i] < smallest) {
            smallest = arr[i];
        }
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }
    
    // Print the smallest and largest elements
    printf("%d %d", smallest, largest);
    
    return 0;
}

#include <stdlib.h>

