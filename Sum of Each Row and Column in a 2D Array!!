#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c); // Read number of rows and columns

    int arr[r][c];

    // Read the elements of the array
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &arr[i][j]);
        }
    }

    // --- Row-wise sums ---
    for (int i = 0; i < r; i++) {
        int rowSum = 0;
        for (int j = 0; j < c; j++) {
            rowSum += arr[i][j];
        }
        printf("%d\n", rowSum);
    }

    // --- Column-wise sums ---
    for (int j = 0; j < c; j++) {
        int colSum = 0;
        for (int i = 0; i < r; i++) {
            colSum += arr[i][j];
        }
        printf("%d\n", colSum);
    }

    return 0;
}
