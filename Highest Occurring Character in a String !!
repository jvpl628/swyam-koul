#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>
#include <string.h>

int main() {
    char str[1001];
    int freq[256] = {0};  // frequency array for all ASCII characters

    // Input string (including spaces)
    fgets(str, sizeof(str), stdin);

    // Count frequency of each character
    for (int i = 0; str[i] != '\0'; i++) {
        freq[(unsigned char)str[i]]++;
    }

    // Find character with highest frequency
    int maxFreq = -1;
    char maxChar;

    for (int i = 0; str[i] != '\0'; i++) {
        if (freq[(unsigned char)str[i]] > maxFreq) {
            maxFreq = freq[(unsigned char)str[i]];
            maxChar = str[i];
        }
    }

    // Output
    printf("%c occurs %d times", maxChar, maxFreq);

    return 0;
}
