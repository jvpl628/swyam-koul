#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int N;
    long long factorial = 1; // long long is used for large numbers

    // Read the input
    scanf("%d", &N);

    // Calculate factorial using a loop
    for (int i = 1; i <= N; i++) {
        factorial = factorial * i;
    }

    // Print the result
    printf("%lld", factorial);

    return 0;
