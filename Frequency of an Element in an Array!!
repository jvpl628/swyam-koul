#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int n, key, count = 0;
    
    // Input number of elements
    scanf("%d", &n);
    
    int arr[n];
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Input the key whose frequency is to be counted
    scanf("%d", &key);
    
    // Count frequency of key
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            count++;
        }
    }
    
    // Output the frequency
    printf("%d", count);
    
    return 0;
}
