#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    int seen[256] = {0};  // To track ASCII characters (case-sensitive)

    // Input
    fgets(str, sizeof(str), stdin);

    // Output original order without duplicates
    for (int i = 0; str[i] != '\0'; i++) {
        unsigned char ch = str[i];
        if (!seen[ch]) {        // First time seen
            printf("%c", ch);
            seen[ch] = 1;
        }
    }

    return 0;
}
