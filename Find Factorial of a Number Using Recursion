#include <stdio.h>
#include <string.h>
#include <math.h>
int factorial(int num) {

    if (num == 0)  

        return 1;

    else

        return num * factorial(num - 1);

}

int main() {

    int num;

    scanf("%d", &num);

    int fact = factorial(num);

    printf("%d", fact);

    return 0;

}
