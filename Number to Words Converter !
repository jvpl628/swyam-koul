#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int n, digit;
    int digits[20];  // to store digits temporarily
    int count = 0;

    // Read input
    scanf("%d", &n);

    // Handle negative numbers
    if (n < 0) {
        n = -n;
    }

    // Special case for 0
    if (n == 0) {
        printf("Zero\n");
        return 0;
    }

    // Extract digits and store in array (reverse order)
    while (n > 0) {
        digits[count] = n % 10;
        n /= 10;
        count++;
    }

    // Print digits from most significant to least significant
    for (int i = count - 1; i >= 0; i--) {
        digit = digits[i];
        switch (digit) {
            case 0: printf("Zero"); break;
            case 1: printf("One"); break;
            case 2: printf("Two"); break;
            case 3: printf("Three"); break;
            case 4: printf("Four"); break;
            case 5: printf("Five"); break;
            case 6: printf("Six"); break;
            case 7: printf("Seven"); break;
            case 8: printf("Eight"); break;
            case 9: printf("Nine"); break;
        }

        if (i != 0)  // Print space between words, not after last
            printf(" ");
    }

    printf("\n");
    return 0;
}
