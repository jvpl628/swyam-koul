#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

    #include <stdio.h>

int main() {
    int m1, m2, m3, m4, m5;
    double total, average, percentage;

    // Read marks
    if (scanf("%d %d %d %d %d", &m1, &m2, &m3, &m4, &m5) != 5) {
        return 1;  // input error
    }

    // Calculate total
    total = m1 + m2 + m3 + m4 + m5;

    // Calculate average
    average = total / 5.0;

    // Calculate percentage (max total = 500)
    percentage = (total / 500.0) * 100.0;
    printf("%.0f %.2f %.2f\n", total, average, percentage);

    return 0;
}
