#include <stdio.h>

int main() {
    int n, value, pos;
    
    // Input number of elements
    scanf("%d", &n);
    
    int arr[101]; // One extra space for new element
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Input the value to insert
    scanf("%d", &value);
    
    // Input the position (1-based index)
    scanf("%d", &pos);
    
    // Shift elements to the right
    for (int i = n; i >= pos; i--) {
        arr[i] = arr[i - 1];
    }
    
    // Insert the new element
    arr[pos - 1] = value;
    n++; // Increase array size
    
    // Print updated array
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    
    return 0;
}
