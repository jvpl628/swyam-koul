#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);  // Input number of elements

    int arr[N];       // Declare array of size N
    int sum = 0;      // Variable to store sum

    // Input array elements
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];  // Add each element to sum
    }

    // Output the total sum
    printf("%d\n", sum);

    return 0;
}
