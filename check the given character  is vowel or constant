#include <stdio.h> 
int main() { 
    char ch; 
    printf("Enter character: "); 
    scanf("%d", &ch); 
    if(ch== 'a' || ch== 'e' || ch== 'o' || ch== 'i' || ch== 'u' || 
        ch== 'A' || ch== 'E' || ch== 'O' || ch== 'I' || ch== 'U') { 
            printf("It is vowel %c \n", ch);
        } 
        else { 
            printf("It is consonant %c \n", ch);
        }


    return 0;
}
