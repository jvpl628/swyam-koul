include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int n;
    
    // Input number of elements
    scanf("%d", &n);
    
    int arr[n], freq[n];
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        freq[i] = -1;  // Initialize frequency array
    }
    
    // Count frequency of each unique element
    for (int i = 0; i < n; i++) {
        if (freq[i] == 0)  // Already counted
            continue;
            
        int count = 1;
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                count++;
                freq[j] = 0;  // Mark duplicate as counted
            }
        }
        freq[i] = count;
    }
    
    // Print elements and their frequencies
    for (int i = 0; i < n; i++) {
        if (freq[i] != 0) {
            printf("%d: %d\n", arr[i], freq[i]);
        }
    }
    
    return 0;
}
