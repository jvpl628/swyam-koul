#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


#include <stdio.h>

int main() {
    int n, key, found = 0;
    
    // Input number of elements
    scanf("%d", &n);
    
    int arr[n];
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Input the key to search for
    scanf("%d", &key);
    
    // Search for the key
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            printf("Element found at position %d", i + 1);
            found = 1;
            break;
        }
    }
    
    // If not found
    if (!found) {
        printf("Element not found");
    }
    
    return 0;
}
