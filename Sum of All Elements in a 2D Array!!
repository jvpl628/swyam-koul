#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int r, c;
    scanf("%d %d", &r, &c); // Read number of rows and columns

    int arr[r][c];
    int sum = 0;

    // Read elements and calculate sum
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &arr[i][j]);
            sum += arr[i][j]; // Add to sum while reading
        }
    }

    // Print the total sum
    printf("%d", sum);

    return 0;
}
