#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n); // Read number of elements
    int arr[n];

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); // Read array elements
    }

    int count = 0;

    for (int i = 0; i < n; i++) {
        int freq = 0;

        // Skip already processed elements
        int alreadyCounted = 0;
        for (int k = 0; k < i; k++) {
            if (arr[k] == arr[i]) {
                alreadyCounted = 1;
                break;
            }
        }

        if (alreadyCounted)
            continue;

        // Count occurrences of arr[i]
        for (int j = 0; j < n; j++) {
            if (arr[i] == arr[j])
                freq++;
        }

        // If the element appears more than once, it's a duplicate
        if (freq > 1)
            count++;
    }

    printf("%d", count);
    return 0;
}
